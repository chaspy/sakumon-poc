{
  "name": "sakumon-poc",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "predev": "pnpm --filter @sakumon/schemas --filter @sakumon/workflows --filter @sakumon/pdf build",
    "dev": "pnpm --filter @sakumon/schemas --filter @sakumon/workflows --filter @sakumon/pdf --parallel dev & pnpm --filter @sakumon/api --filter @sakumon/web --parallel dev",
    "build": "pnpm -r build",
    "typecheck": "pnpm -r typecheck"
  },
  "devDependencies": {
    "typescript": "^5.4.0"
  }
}
